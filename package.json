{
  "private": true,
  "name": "sqs-viewer-api",
  "version": "1.0.0",
  "description": "sqs viewer",
  "main": "./dist/index",
  "scripts": {
    "eslint": "eslint '**/*.{ts,js}' --ignore-pattern node_modules/; exit 0",
    "eslint:fix": "eslint '**/*.{ts,js}' --fix --ignore-pattern node_modules/; exit 0",
    "compile:all": "rm -rf dist && npm run compile:public && npm run compile:ts",
    "compile:ts": "tsc -p ./tsconfig.src.json",
    "compile:public": "mkdir -p ./dist && cp -r ./src/public ./dist",
    "dev": "concurrently -n \"src,public\" \"npm run watch:src\" \"npm run watch:public\"",
    "watch:src": "rm -rf dist && tsc-watch --noClear -p tsconfig.src.json --onSuccess \"npm start\"",
    "watch:public": "nodemon --watch ./src/public --ext '.html .css .js' --exec 'npm run compile:public'",
    "start": "node .",
    "publish": "npm run compile:all && docker build -t solfu/$npm_package_name:$npm_package_version . && docker push solfu/$npm_package_name:$npm_package_version",
    "test": "npm run stop && ts-node ./npm_scripts/start.ts",
    "stop": "docker-compose down -v --remove-orphans"
  },
  "keywords": [],
  "author": "Soluzioni Futura <info@soluzionifutura.it>",
  "license": "ISC",
  "devDependencies": {
    "@soluzioni-futura/eslint-config-soluzioni-futura": "^1.4.0",
    "@types/chai": "^4.2.16",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/tsc-watch": "^4.2.0",
    "@types/yargs-parser": "^20.2.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "chai": "^4.3.4",
    "concurrently": "^6.0.1",
    "eslint": "^7.24.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4",
    "yaml": "^1.10.2",
    "yargs-parser": "^20.2.7"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "aws-sdk": "^2.884.0",
    "express": "^4.17.1"
  }
}
